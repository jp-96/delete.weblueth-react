{
  "name": "weblueth-react",
  "version": "0.1.0",
  "description": "My first react typescript package",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jp-96/weblueth-react.git"
  },
  "targets": {
    "sample": {
      "source": "sample/index.html",
      "isLibrary": false,
      "distDir": "./.build"
    }
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint",
    "build-sample": "npx parcel build --no-cache --no-source-maps --target sample"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "awesome-project"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@xstate/react": "^3.2.1",
    "microbit-web-bluetooth": "^0.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "xstate": "^4.37.0"
  }
}
